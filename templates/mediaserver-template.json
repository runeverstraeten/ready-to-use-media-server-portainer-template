[
    {
        "type": 1,
        "title": "Plex Media Server",
        "name": "plex",
        "description": "Transform your home into a cinematic oasis with your very own media server. Stream Movies & TV Shows.",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Plex/icon.png",
        "image": "lscr.io/linuxserver/plex:latest",
        "categories": ["Media"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "network": "host",
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" },
            { "name": "VERSION", "label": "Version", "default": "docker", "description": "Docker or latest" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/plex/config", "label": "Config Path" },
            { "container": "/media", "bind": "/path/to/media", "label": "Media Path" }
        ],
        "note": "Plex draait in 'Host' netwerk modus voor de beste DLNA/Discovery werking. Poort mapping is hierdoor niet nodig."
    },
    {
        "type": 1,
        "title": "Overseerr",
        "name": "overseerr",
        "description": "Overseerr is a free and open source software application for managing requests for your media library. It integrates with your existing services, such as Sonarr, Radarr, and Plex!",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Overseerr/icon.png",
        "image": "lscr.io/linuxserver/overseerr:latest",
        "categories": ["Media", "Requests"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "5055/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/overseerr/config", "label": "Config Path" }
        ]
    },
    {
        "type": 1,
        "title": "Tautulli",
        "name": "tautulli",
        "description": "Tautulli is a 3rd party application that you can run alongside your Plex Media Server to monitor activity and track various statistics.",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Tautulli/icon.png",
        "image": "lscr.io/linuxserver/tautulli:latest",
        "categories": ["Media", "Monitoring"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "8181/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/tautulli/config", "label": "Config Path" }
        ]
    },
    {
        "type": 1,
        "title": "Radarr",
        "name": "radarr",
        "description": "Radarr is a movie collection manager for Usenet and BitTorrent users. It can monitor multiple RSS feeds for new movies and will interface with clients and indexers.",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Radarr/icon.png",
        "image": "lscr.io/linuxserver/radarr:latest",
        "categories": ["Media", "Downloaders"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "7878/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/radarr/config", "label": "Config Path" },
            { "container": "/media", "bind": "/path/to/media", "label": "Media Path (Movies)" },
            { "container": "/downloads", "bind": "/path/to/downloads", "label": "Downloads Path" }
        ]
    },
    {
        "type": 1,
        "title": "Sonarr",
        "name": "sonarr",
        "description": "Sonarr is a PVR for Usenet and BitTorrent users. It can monitor multiple RSS feeds for new episodes of your favorite shows and will grab, sort and rename them.",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Sonarr/icon.png",
        "image": "lscr.io/linuxserver/sonarr:latest",
        "categories": ["Media", "Downloaders"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "8989/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/sonarr/config", "label": "Config Path" },
            { "container": "/media", "bind": "/path/to/media", "label": "Media Path (TV Shows)" },
            { "container": "/downloads", "bind": "/path/to/downloads", "label": "Downloads Path" }
        ]
    },
    {
        "type": 1,
        "title": "Bazarr",
        "name": "bazarr",
        "description": "Bazarr is a companion application to Sonarr and Radarr. It manages and downloads subtitles based on your requirements.",
        "logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/bazarr-icon.png",
        "image": "lscr.io/linuxserver/bazarr:latest",
        "categories": ["Media", "Subtitles"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "6767/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/bazarr/config", "label": "Config Path" },
            { "container": "/media", "bind": "/path/to/media", "label": "Media Path" }
        ]
    },
    {
        "type": 1,
        "title": "Transmission",
        "name": "transmission",
        "description": "Transmission is a fast, easy, and free BitTorrent client. It comes with a simple web interface.",
        "logo": "https://upload.wikimedia.org/wikipedia/commons/thumb/4/46/Transmission_Icon.svg/2048px-Transmission_Icon.svg.png",
        "image": "lscr.io/linuxserver/transmission:latest",
        "categories": ["Downloaders"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "9091/tcp",
            "51413/tcp",
            "51413/udp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/transmission/config", "label": "Config Path" },
            { "container": "/downloads", "bind": "/path/to/downloads", "label": "Downloads Path" },
            { "container": "/watch", "bind": "/path/to/watch", "label": "Watch Path" }
        ]
    },
    {
        "type": 1,
        "title": "Jackett",
        "name": "jackett",
        "description": "Jackett works as a proxy server: it translates queries from apps (Sonarr, Radarr, etc) into tracker-site-specific http queries.",
        "logo": "https://cdn.jsdelivr.net/gh/IceWhaleTech/CasaOS-AppStore@main/Apps/Jackett/icon.png",
        "image": "lscr.io/linuxserver/jackett:latest",
        "categories": ["Downloaders", "Indexers"],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            "9117/tcp"
        ],
        "env": [
            { "name": "PUID", "label": "PUID", "default": "1000", "description": "User ID" },
            { "name": "PGID", "label": "PGID", "default": "1000", "description": "Group ID" },
            { "name": "TZ", "label": "Timezone", "default": "Europe/Brussels" }
        ],
        "volumes": [
            { "container": "/config", "bind": "/path/to/jackett/config", "label": "Config Path" },
            { "container": "/downloads", "bind": "/path/to/downloads", "label": "Downloads Path" }
        ]
    }
]
